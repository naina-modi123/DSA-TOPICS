class Solution {
    public int maxProduct(int[] nums) {
        int n = nums.length;
        int maxi = Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            for(int j = i;j<n;j++){
                int product = 1;
                for(int k = i;k<=j;k++){
                    product = product * nums[k];
                }
                maxi = Math.max(maxi , product);
            }
        }
        return maxi;
    }
}
