import java.util.*;
class Node{
    int data;
    Node next;
    Node(int x){
        data = x;
        next = null;
    }
}
class LinkedList{
    public static Node delTail(Node head){
       if(head == null){
           return null;
       }
       if(head.next == null){
           return null;
       }
       Node temp = head;
       while(temp.next.next != null){
           temp = temp.next;
       }
       temp.next = null;
       return head;
       
    }
    public static void printlist(Node head){
        Node curr = head;
        while(curr != null){
            System.out.print(curr.data+" ");
            curr = curr.next;
        }
        System.out.println();
    }
    public static void main(String args[]){
        Node head = new Node(10);
        head.next = new Node(20);
        head.next.next = new Node(30);
        printlist(head);
        head = delTail(head);
        printlist(head);
    }
}
